{
  "name": "prompt-context",
  "version": "0.2.0-beta.0",
  "description": "An MCP protocol that helps AI agents efficiently remember and utilize previous conversation context.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "prompt-context": "dist/mcp-server.bundle.js"
  },
  "files": [
    "dist",
    "models",
    "README.md",
    "README_KOR.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "build:webpack": "webpack && chmod +x dist/mcp-server.bundle.js",
    "build:standalone": "npm run clean && npm run build:webpack",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "clean": "rm -rf dist",
    "prepare": "npm run build",
    "postbuild": "chmod +x dist/mcp-server.bundle.js",
    "start:mcp": "node dist/mcp-server.bundle.js",
    "start:standalone": "node dist/mcp-server.bundle.js",
    "mcp": "ts-node src/main.ts",
    "dev": "webpack --watch --mode=development",
    "test:complex-scenario": "npx ts-node src/__tests__/integration-tests/complex-development-scenario.test.ts",
    "test:react-ai": "npx ts-node src/__tests__/integration-tests/react-ai-development.test.ts",
    "test:all": "npm run test:complex-scenario && npm run test:react-ai",
    "test": "npm run lint && npm run test:all",
    "deploy:beta": "npm run build && npm publish --tag beta",
    "deploy:latest": "npm run build && npm publish --tag latest"
  },
  "keywords": [
    "ai",
    "context",
    "memory",
    "llm",
    "agent",
    "mcp",
    "modelcontextprotocol",
    "model-context-protocol",
    "prompt-context"
  ],
  "author": "Axistant",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.8.0",
    "dotenv": "^16.4.1",
    "fs-extra": "^11.2.0",
    "graphology": "^0.26.0",
    "graphology-gexf": "^0.13.2",
    "hnswlib-node": "^3.0.0",
    "jsonfile": "^6.1.0",
    "lodash": "^4.17.21",
    "onnxruntime-node": "^1.21.0",
    "readline": "^1.3.0",
    "uuid": "^11.1.0",
    "winston": "^3.11.0",
    "zod": "^3.22.4",
    "zod-to-json-schema": "^3.22.3",
    "@schemathesis/jest": "^2.1.0",
    "@types/minio": "^7.1.1",
    "@xenova/transformers": "^2.15.1",
    "fastify-socket.io": "^5.0.0",
    "fastify-static": "^4.7.0",
    "fp-ts": "^2.16.2"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/jsonfile": "^6.1.1",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.11.19",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^4.3.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "node-loader": "^2.1.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "webpack-node-externals": "^3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Nhahan/prompt-context.git"
  },
  "bugs": {
    "url": "https://github.com/Nhahan/prompt-context/issues"
  },
  "homepage": "https://github.com/Nhahan/prompt-context",
  "publishConfig": {
    "tag": "beta"
  },
  "directories": {
    "doc": "docs"
  }
}
