{
  "name": "prompt-context",
  "version": "0.1.1-beta.18",
  "description": "An MCP protocol that helps AI agents efficiently remember and utilize previous conversation context.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "prompt-context": "dist/mcp-server.js"
  },
  "scripts": {
    "build": "npm run clean && tsc && chmod +x dist/mcp-server.js",
    "lint": "eslint src/**/*.ts",
    "prepublish": "npm run build",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "postbuild": "chmod +x dist/mcp-server.js",
    "start:mcp": "node dist/mcp-server.js",
    "mcp": "ts-node src/mcp-server.ts",
    "cli": "node dist/cli.js",
    "dev": "tsc -w",
    "test": "jest"
  },
  "keywords": [
    "ai",
    "context",
    "memory",
    "llm",
    "agent",
    "mcp",
    "model-context-protocol"
  ],
  "author": "Axistant",
  "license": "MIT",
  "dependencies": {
    "@huggingface/inference": "^2.6.4",
    "@modelcontextprotocol/sdk": "^0.1.0",
    "@xenova/transformers": "^2.16.1",
    "axios": "^1.6.7",
    "dotenv": "^16.4.1",
    "fs-extra": "^11.2.0",
    "jsonfile": "^6.1.0",
    "langchain": "^0.1.17",
    "lodash": "^4.17.21",
    "neo4j-driver": "^5.17.0",
    "onnxruntime-node": "^1.17.0",
    "readline": "^1.3.0",
    "sqlite3": "^5.1.7",
    "typescript-json-schema": "^0.62.0",
    "uuid": "^9.0.1",
    "vectra": "^0.6.1",
    "winston": "^3.11.0",
    "zod": "^3.22.4",
    "zod-to-json-schema": "^3.22.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.11",
    "@types/jsonfile": "^6.1.1",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.11.10",
    "@types/supertest": "^6.0.3",
    "@types/sqlite3": "^3.1.11",
    "@types/uuid": "^9.0.7",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "eslint": "^8.48.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.3",
    "rimraf": "^5.0.5",
    "supertest": "^7.1.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Nhahan/prompt-context.git"
  },
  "bugs": {
    "url": "https://github.com/Nhahan/prompt-context/issues"
  },
  "homepage": "https://github.com/Nhahan/prompt-context",
  "publishConfig": {
    "tag": "beta"
  },
  "directories": {
    "doc": "docs"
  }
